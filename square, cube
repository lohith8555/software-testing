package poorna1;
import static org.junit.Assert.assertTrue;
import java.util.*;
public class poorna7{
    public static void main(String[] args){
        Scanner aa = new Scanner(System.in);
        System.out.println("enter the number");
        int n = aa.nextInt();
        int a,b;
        a=n*n;
        b=n*n*n;
        System.out.println("the square root"+a);
        System.out.println("the cube root"+b);
        assertTrue(4==a);
        assertTrue(8==b);
    }
}
